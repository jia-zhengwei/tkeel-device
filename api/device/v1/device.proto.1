
syntax = "proto3";

package api.device.v1;

import "google/api/annotations.proto";
option go_package = "device/api/device/v1;v1";
option java_multiple_files = true;
option java_package = "api.device.v1";

service Device {
	rpc CreateDevice (CreateDeviceRequest) returns (CreateDeviceResponse) {
		option (google.api.http) = {
			post : "/device"
			body : "*"
		};
	};
	rpc UpdateDevice (UpdateDeviceRequest) returns (UpdateDeviceResponse) {
		option (google.api.http) = {
			put : "/device/{uid}"
			body : "*"
		};
	};
	rpc DeleteDevice (DeleteDeviceRequest) returns (DeleteDeviceResponse) {
		option (google.api.http) = {
			delete : "/device/{uid}"
		};
	};
	rpc GetDevice (GetDeviceRequest) returns (GetDeviceResponse) {
		option (google.api.http) = {
			get : "/device/{uid}"
		};
	};
	rpc ListDevice (ListDeviceRequest) returns (ListDeviceResponse) {
		option (google.api.http) = {
			get : "/device"
		};
	};
}

message DeviceObject {
	string uid = 1;
}

message CreateDeviceRequest { DeviceObject obj = 1; }
message CreateDeviceResponse {}

message UpdateDeviceRequest {
	DeviceObject obj = 1;
	string uid = 2;
}
message UpdateDeviceResponse {}

message DeleteDeviceRequest { string uid = 1; }
message DeleteDeviceResponse { DeviceObject obj = 1; }

message GetDeviceRequest { string uid = 1;}
message GetDeviceResponse { DeviceObject obj = 1; }

message ListDeviceRequest {}
message ListDeviceResponse { repeated DeviceObject objList = 1; }

